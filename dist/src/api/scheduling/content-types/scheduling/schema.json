{
    "kind": "collectionType",
    "collectionName": "schedulings",
    "info": {
        "singularName": "scheduling",
        "pluralName": "schedulings",
        "displayName": "Scheduling",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "schedulingTitle": {
            "type": "string",
            "required": true
        },
        "court_availability": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::court-availability.court-availability",
            "inversedBy": "schedulings"
        },
        "date": {
            "type": "date",
            "required": true
        },
        "users": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "plugin::users-permissions.user",
            "inversedBy": "schedulings"
        },
        "valuePayed": {
            "type": "decimal",
            "min": 0
        },
        "payedStatus": {
            "type": "boolean",
            "default": false,
            "required": true
        },
        "owner": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.user",
            "inversedBy": "schedulings_owner"
        },
        "payDay": {
            "type": "date",
            "required": false
        },
        "user_payments": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::user-payment.user-payment",
            "mappedBy": "scheduling"
        },
        "cancelMessage": {
            "type": "text",
            "maxLength": 200
        },
        "activationKey": {
            "type": "string",
            "maxLength": 4
        },
        "activated": {
            "type": "boolean",
            "default": false,
            "required": false
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "canceled",
                "payed",
                "waiting"
            ],
            "default": "waiting",
            "required": true
        },
        "serviceRate": {
            "type": "decimal",
            "default": 0,
            "min": 0,
            "max": 5
        }
    }
}
