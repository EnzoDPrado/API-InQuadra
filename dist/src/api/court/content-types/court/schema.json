        },
        "establishment": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::establishment.establishment",
            "inversedBy": "courts"
        },
        "court_type": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::court-type.court-type",
            "inversedBy": "courts"
        },
        "court_availabilities": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::court-availability.court-availability",
