{
  "kind": "collectionType",
  "collectionName": "court_availabilities",
  "info": {
    "singularName": "court-availability",
    "pluralName": "court-availabilities",
    "displayName": "CourtAvailability",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "weekDay": {
      "type": "enumeration",
      "enum": [
        "Monday",
        "Tuesday",
        "Wednesday",
        "Thursday",
        "Friday",
        "Saturday",
        "Sunday",
        "SpecialDays"
      ]
    },
    "startsAt": {
      "type": "time",
      "required": true
    },
    "endsAt": {
      "type": "time",
      "required": true
    },
    "value": {
      "type": "decimal",
      "required": true
    },
    "dayUseService": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "court": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::court.court",
      "inversedBy": "court_availabilities"
    },
    "status": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "schedulings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::scheduling.scheduling",
      "mappedBy": "court_availability"
    }
  }
}
